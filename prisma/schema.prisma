generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(cuid())
  email String @unique
  displayName String
  createdAt DateTime @default(now())
  undatedAt DateTime @updatedAt
  userBooks UserBook[]

  @@map("users")
}